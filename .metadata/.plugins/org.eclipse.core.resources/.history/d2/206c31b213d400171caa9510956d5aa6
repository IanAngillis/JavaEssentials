package opdracht;

public class Rectangle {
	
	//Public: de variabelen zijn direct aanspreekbaar en aanpasbaar
	public int x;
	public int y;
	public int height;
	public int width;

	// Main, maakt een rect object aan
	public static void main(String[] args) {

		System.out.println("This program uses a rectangle");
		Rectangle rect = new Rectangle();
		System.out.println("Height: "+rect.height);
		System.out.println("Height: "+rect.width);
		System.out.println("Position: " + "(" + rect.x + "," + rect.y + ")");
	}
}
